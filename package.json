{
  "name": "normalize.cssinjs",
  "version": "1.0.0",
  "description": "Normalize.css in CSS-in-JS format",
  "main": "src/index.js",
  "scripts": {
    "test": "mocha --require @babel/polyfill --require @babel/register 'src/*.test.js'",
    "test:watch": "mocha -w --require @babel/polyfill --require @babel/register 'src/*.test.js'",
    "prebuild": "rimraf dist",
    "build": "babel src --out-dir dist --ignore 'src/*.test.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/J-Kallunki/normalize.cssinjs.git"
  },
  "keywords": [
    "normalize",
    "css",
    "reset",
    "cssinjs"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Joonas Kallunki <jkallunki@gmail.com> (https://jkallunki.net/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/J-Kallunki/normalize.cssinjs/issues"
  },
  "homepage": "https://github.com/J-Kallunki/normalize.cssinjs#readme",
  "dependencies": {
    "@csstools/normalize.css": "9.0.1",
    "@jkallunki/css-in-to-js": "1.0.1",
    "husky": "1.1.4",
    "lint-staged": "8.0.4",
    "prettier": "1.15.2"
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.6",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.1.6",
    "@babel/register": "7.0.0",
    "babel-plugin-inline-import": "3.0.0",
    "chai": "4.2.0",
    "mocha": "5.2.0",
    "mocha-loader": "2.0.0",
    "rimraf": "2.6.2",
    "webpack": "4.25.1",
    "webpack-cli": "3.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  }
}
